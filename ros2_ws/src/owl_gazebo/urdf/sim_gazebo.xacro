<?xml version="1.0"?>
<robot name="owl_robot" xmlns:xacro="http://wiki.ros.org/xacro">


    <xacro:include filename="$(find owl_description)/urdf/robot.urdf"/>    
    <xacro:include filename="$(find owl_gazebo)/gazebo_plugins.xacro"/>    

    <!-- OWL 6.5 Robot-->
    <xacro:owl_arm parent="world">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
    </xacro:owl_arm>

    <!-- OWL 6.5 Gazebo control-->
    <xacro:gazebo_control/>
    
</robot>