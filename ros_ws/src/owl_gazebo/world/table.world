<?xml version="1.0" ?>
<sdf version="1.5">

  <world name="table">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <!-- Table -->
    <include>
      <uri>model://table</uri>
    </include>
  </world>
</sdf>

   <!-- Red Cube -->
        <model name="red_cube">
      <pose>0.5 0 1.07 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>0.2 0.2 0.2</size>
            </box>
          </geometry>
          <material>
            <ambient>1 0 0 1</ambient>
            <diffuse>1 0 0 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.2 0.2 0.2</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>
    <!-- Green Cube -->
